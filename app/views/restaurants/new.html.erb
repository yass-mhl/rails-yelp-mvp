<%= simple_form_for(@restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
  <%= f.input :category, collection: %w(chinese italian japanese french belgian) %>
  <%= f.submit %>
<% end %>

<%= link_to 'back', restaurants_path %>
